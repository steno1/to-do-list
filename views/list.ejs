
<%- include('header'); -%>

<div class="box" id="heading">
<h1>Princeley to-do-list</h1>

<h2><%=listTitle%></h2>
</div>
<div class="box" >

<% for (let i=0; i<newLists.length; i++){%>
<div class="item">
<input type="checkbox" >
<p><%= newLists[i] %></p>
</div>

<% }%>
<form class="item" action="/" method="post">
